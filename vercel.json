{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": "public",
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/api-gateway.js" },
    {
      "source": "/((?!assets|api|manifest\\.webmanifest|service-worker\\.js|favicon\\.svg).*)",
      "destination": "/index.html"
    }
  ]
}
